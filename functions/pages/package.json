{
  "name": "baidaohui-pages",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler pages dev",
    "build": "tsc",
    "deploy": "wrangler pages deploy",
    "lint": "eslint api/**/*.ts",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20241218.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.0.0",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "@kit/eslint-config": "workspace:*",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.0.0",
    "@types/qrcode": "^1.5.5",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.0.0",
    "wrangler": "^4.19.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>"],
    "testMatch": ["**/*.test.ts"],
    "collectCoverageFrom": [
      "functions/**/*.ts",
      "!functions/**/*.test.ts"
    ]
  }
}